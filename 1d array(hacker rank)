// 1d array(hacker rank)
//n=6
//1 2 3 4 5 6
//21


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n,i;
    scanf("%d",&n);
    
    int arr[n];
    for (int i=0;i<n;i++) {
    scanf("%d",&arr[i]);
}
        int sum=0;
        for(int i=0;i<n;i++){
        sum=sum+arr[i];
        
    }  
    printf("%d",sum);
    return 0;
}
